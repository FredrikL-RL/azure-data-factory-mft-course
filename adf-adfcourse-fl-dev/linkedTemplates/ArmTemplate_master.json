{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-adfcourse-fl-dev"
		},
		"ls_adfcourse_dls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsadfcoursefldev001.dfs.core.windows.net/"
		},
		"ls_adfcourse_dls_logging_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsadfcourselogfldev001.dfs.core.windows.net/"
		},
		"ls_adfcourse_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-adfcourse-fl-dev-001.vault.azure.net/"
		},
		"ls_adfcourse_sftp_inbound_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "stadfcoursesftpfldev001.blob.core.windows.net"
		},
		"ls_adfcourse_sftp_inbound_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "stadfcoursesftpfldev001.inbound.adfcourse"
		},
		"ls_adfcourse_sftp_outbound_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "stadfcoursesftpfldev001.blob.core.windows.net"
		},
		"ls_adfcourse_sftp_outbound_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "stadfcoursesftpfldev001.outbound.adfcourse"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-adfcourse-fl-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adfcourse_dls_properties_typeProperties_url": {
						"value": "[parameters('ls_adfcourse_dls_properties_typeProperties_url')]"
					},
					"ls_adfcourse_dls_logging_properties_typeProperties_url": {
						"value": "[parameters('ls_adfcourse_dls_logging_properties_typeProperties_url')]"
					},
					"ls_adfcourse_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_adfcourse_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_adfcourse_sftp_inbound_properties_typeProperties_host": {
						"value": "[parameters('ls_adfcourse_sftp_inbound_properties_typeProperties_host')]"
					},
					"ls_adfcourse_sftp_inbound_properties_typeProperties_userName": {
						"value": "[parameters('ls_adfcourse_sftp_inbound_properties_typeProperties_userName')]"
					},
					"ls_adfcourse_sftp_outbound_properties_typeProperties_host": {
						"value": "[parameters('ls_adfcourse_sftp_outbound_properties_typeProperties_host')]"
					},
					"ls_adfcourse_sftp_outbound_properties_typeProperties_userName": {
						"value": "[parameters('ls_adfcourse_sftp_outbound_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}